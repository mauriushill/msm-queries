<h1>List of all actors</h1>
<hr>
<table border="1">
<tr>
<th>ID</th>
<th>Image</th>
<th>Name</th>
<th>Dob</th>
<th></th>
</tr>

<% Actor.all.each do |actor|%>

<tr>
<td><%= actor.id %></td>
<td><img src="<%= actor.image %>" width="100"</td>
<td><%= actor.name %></td>
<td><%= actor.dob %></td>
<td><a href="/actors/<%= actor.id %>">Show details</details></td>
</tr>

<% end %>
</table>
